{
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "2.0.0",
  "permissions": ["declarativeContent", "activeTab", "storage", "http://*/browse/*", "https://*/browse/*"],
  "content_scripts": [{
    "all_frames": false,
    "js": ["js/jquery-3.1.0.min.js", "js/jquery-ui.min.js"],
    "matches": ["http://*/browse/*", "https://*/browse/*"],
    "run_at": "document_end"
  }],
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "options.html",
  "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "author": "gaerae.com",
  "homepage_url": "https://github.com/gaerae/jira-subtask-sortable"
}
